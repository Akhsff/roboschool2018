<?xml version="1.0"?>
<!-- Load emtpy world with car and keyboard controlling -->
<launch>
    <!-- Gazebo -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">    	
        <arg name="paused" value="True"/>
        <arg name="use_sim_time" value="True"/>
    </include>

    <!-- Load xacro, convert to URDF and spawn-->
    <include file="$(find roboschool_common)/launch/spawn.launch">
        <arg name="filename" value="$(find car_description)/urdf/car.xacro"/>
    </include>

    <!-- Keyboard control -->
    <include file="$(find roboschool_common)/launch/keyboard.launch"/>
</launch>